{
  "name": "lrna-qc",
  "title": "long RNAseq QC metrics",
  "summary": "lrna-qc",
  "dxapi": "1.0.0",
  "version": "0.0.1",
  "categories": [
    "ENCODE"
  ],
  "inputSpec": [
    {
      "name": "rep1_quants",
      "label": "Replicate 1 RSEM quantification files",
      "class": "array:file",
      "patterns": ["*.tsv"],
      "optional": false
    },
    {
      "name": "rep2_quants",
      "label": "Replicate 2 RSEM quantification files",
      "class": "array:file",
      "patterns": ["*.tsv"],
      "optional": false
    }
  ],
  "outputSpec": [
    {
      "name": "MADs",
      "label": "Mean average deviations of log ratios",
      "class": "array:float"
    },
    {
      "name": "SADs",
      "label": "SDs of log ratios",
      "class": "array:float"
    },
    {
      "name": "PCCs",
      "label": "Pearson correlation coefficients",
      "class": "array:float"
    },
    {
      "name": "SCCs",
      "label": "Spearman correlation coefficients",
      "class": "array:float"
    },
    {
      "name": "MAD_plots",
      "label": "Plot of mean average deviation of log ratios",
      "class": "array:file"
    }
  ],
  "runSpec": {
    "interpreter": "python2.7",
    "file": "src/lrna-qc.py",
    "execDepends": {
      "name": "r-base-core",
      "name": "ghostscript"
    }
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "authorizedUsers": []
}
